<% if images.size > 0 %>
<ul>
  <% images.each do |image| %>
    <li>
      <a href="javascript:void(0)" onclick="<%="ImageDialog.insert_uploaded_image('#{image.image.url(:medium)}', '#{image.image.name}');" %>">
      <%= image_tag( image.image.url(:thumb) ) %></a>
    </li>
  <% end %>
</ul>
<% else %>
 No Images Uploaded Yet.
<% end %>